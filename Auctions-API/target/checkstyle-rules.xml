<?xml version="1.0"?>
<!DOCTYPE module PUBLIC "-//Puppy Crawl//DTD Check Configuration 1.3//EN"
        "http://www.puppycrawl.com/dtds/configuration_1_3.dtd">
<module name="Checker"><module name="FileTabCharacter"/>
<module name="TreeWalker"><module name="OuterTypeFilename"/>
<module name="EqualsHashCode"/>
<module name="LeftCurly"><property name="option" value="eol"/>
</module>
<module name="DefaultComesLast"/>
<module name="GenericWhitespace"/>
<module name="ClassTypeParameterName"><property name="format" value="^[A-Z][a-zA-Z0-9]*$"/>
</module>
<module name="LocalFinalVariableName"/>
<module name="LocalVariableName"><property name="format" value="^[a-z_][a-zA-Z0-9]*$"/>
</module>
<module name="MemberName"><property name="format" value="^[a-z_][a-zA-Z0-9]*$"/>
</module>
<module name="MethodName"><property name="format" value="^[a-z][a-zA-Z0-9]*$"/>
</module>
<module name="ParameterName"/>
</module>
<module name="Header"><property name="headerFile" value="config/checkstyle/header.txt"/>
<property name="fileExtensions" value="java"/>
</module>
</module>
